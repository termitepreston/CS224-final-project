<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="changelog-0.1" author="alazar">
        <createTable tableName="greetings">
            <column name="id" autoIncrement="true" type="int">
                <constraints primaryKey="true"/>
            </column>
            <column name="val" type="text">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <insert tableName="greetings">
            <column name="val">Hello</column>
        </insert>
        <insert tableName="greetings">
            <column name="val">Hola</column>
        </insert>
        <insert tableName="greetings">
            <column name="val">Halo</column>
        </insert>
    </changeSet>
</databaseChangeLog>